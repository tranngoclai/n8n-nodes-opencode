{
	"compilerOptions": {
		"strict": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "es2019",
		"lib": ["es2019", "es2020", "es2022.error"],
		"removeComments": true,
		"useUnknownInCatchVariables": false,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"strictNullChecks": true,
		"preserveConstEnums": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"incremental": true,
		"declaration": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"types": ["node"],
		"outDir": "./dist/"
	},
	"include": [
		"credentials/**/*.ts",
		"nodes/Antigravity/**/*.ts",
		"nodes/Antigravity/**/*.json",
		"nodes/Antigravity/**/*.svg",
		"package.json"
	],
	"exclude": [
		"dist",
		"node_modules",
		"nodes/Antigravity/account-manager/**/*",
		"nodes/Antigravity/server.js",
		"nodes/Antigravity/index.js"
	]
}
